<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveSignUpServlet</servlet-name>
  	<display-name>SaveSignUpServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveSignUpServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AuthenticationServlet</servlet-name>
  	<display-name>AuthenticationServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.AuthenticationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveAccountServlet</servlet-name>
  	<display-name>SaveAccountServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveAccountServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransferMoneyServlet</servlet-name>
  	<display-name>TransferMoneyServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.TransferMoneyServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransferAuthenticationServlet</servlet-name>
  	<display-name>TransferAuthenticationServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.TransferAuthenticationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogOutServlet</servlet-name>
  	<display-name>LogOutServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.LogOutServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>SaveSignUpServlet</servlet-name>
  	<url-pattern>/SaveSignUpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthenticationServlet</servlet-name>
  	<url-pattern>/AuthenticationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveAccountServlet</servlet-name>
  	<url-pattern>/SaveAccountServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransferMoneyServlet</servlet-name>
  	<url-pattern>/TransferMoneyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransferAuthenticationServlet</servlet-name>
  	<url-pattern>/TransferAuthenticationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogOutServlet</servlet-name>
  	<url-pattern>/LogOutServlet</url-pattern>
  </servlet-mapping>
</web-app>
